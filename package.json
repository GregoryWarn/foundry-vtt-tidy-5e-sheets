{
  "name": "tidy5e-sheet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare-dev": "node prepare-dist-for-dev.js",
    "dev": "npm run prepare-dev && vite",
    "build": "vite build",
    "build-watch": "vite build --watch",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check-errors": "svelte-check --threshold error --tsconfig ./tsconfig.json",
    "check-watch": "svelte-check --watch --tsconfig ./tsconfig.json",
    "check-errors-watch": "svelte-check --threshold error --watch --tsconfig ./tsconfig.json",
    "refresh-docs": "typedoc --options ./typedoc.json",
    "link-create": "node symlink-module-build-dir.js",
    "link-remove": "node symlink-module-build-dir.js --clean",
    "lint-tests": "eslint tests/**/*.ts",
    "lint-tests-fix": "eslint tests/**/*.ts --fix",
    "e2e": "npm run lint-tests && playwright test"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@playwright/test": "^1.48.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "eslint-plugin-playwright": "^2.0.0",
    "fs-extra": "^11.2.0",
    "sass": "^1.79.3",
    "svelte": "^5.10.0",
    "svelte-check": "^4.1.1",
    "terser": "^5.36.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.26.11",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.4.10",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@melloware/coloris": "^0.24.0",
    "colord": "^2.9.3"
  }
}
